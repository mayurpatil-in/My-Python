   "source": [
    "# Initialize a counter\n",
    "counter = 1\n",
    "\n",
    "# Loop while counter is less than or equal to 5\n",
    "while counter <= 5:\n",
    "    print(f\"Counter is at: {counter}\")\n",
    "    counter += 1  # Increment the counter by 1\n",
    "\n",
    "print(\"Loop has ended.\")\n"
   ]
